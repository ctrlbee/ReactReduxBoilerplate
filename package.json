{
  "name": "node-react-redux-boilerplate",
  "version": "0.5.0",
  "description": "A simple boilerplate to get started with a node/react/redux app.",
  "private": false,
  "scripts": {
    "webpackdev": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "startdev": "webpack --watch & nodemon src-server/app.js --exec babel-node",
    "build": "webpack & babel src-server -d dist-server",
    "serve": "node dist-server/app.js"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "body-parser": "^1.15.2",
    "dotenv": "^4.0.0",
    "express": "^4.14.0",
    "express-session": "^1.14.2",
    "express-status-monitor": "^0.1.5",
    "express-validator": "^3.1.2",
    "lodash": "^3.10.1",
    "morgan": "^1.7.0",
    "react": "^15.4.0",
    "react-dom": "^15.4.3",
    "react-promise": "^1.1.3",
    "react-redux": "5.0.0",
    "react-router": "^4.1.0",
    "react-router-dom": "^4.0.0",
    "redux": "^3.7.0",
    "redux-promise": "^0.5.3"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.4",
    "file-loader": "^0.11.2",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  }
}
